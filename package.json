{
  "name": "WebAngularBackendManagerApp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-proxy": "ng serve --proxy-config src/proxy.conf.json",
    "serve-prod-80": "ng serve --host 0.0.0.0 --c --port 80",
    "start": "ng serve --host 0.0.0.0 --c --port 4444",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "build-prod-serve": "ng build --prod && nodemon start server.js",
    "serve": "nodemon start server.js",
    "build-prod-base": "ng build --prod --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock-generate": "node ./server/generate.js > ./server/database.json",
    "mock-server": "json-server --watch ./server/database.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
